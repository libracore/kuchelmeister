 "html": "<!-- letter head content part -->\n<div  id=\"header-html\" class=\"hidden-pdf\" >\n{% set letter_head = frappe.get_doc(\"Letter Head\", \"Kuchelmeister\") %}\n    {% if letter_head %}\n        {{ letter_head.content }}\n    {% else %}\n<p>Letter head Commercial not found. Please define the letter head under print settings</p>\n        {% endif %}\n</div>\n\n\n<!-- header part -->\n<div style=\"display: block\">\n  \n        <div style=\"float: left; background-color: white; width: 30%; margin-top: 7%\">\n            <p > Lieferadresse:<br>\n                <b>{{ doc.customer_name }}</b><br>\n                {% if doc.shipping_address %}{{ doc.shipping_address }}{% endif %}<br>\n                <br>\n                <br>\n                <b> {{ doc.customer_name }} </b> <br>\n                {% if doc.address_display%} {{ doc.address_display }}{% endif %}\n                <br>\n            </p>\n    </div> \n\n\n\n<!-- tier 1 left div -->\n\n    <div style=\"float: left; width: 70%; background-color: white; margin-top: 5%\">\n           <div style=\"display: block;\">\n        <h4 style=\"margin-left: 90px; font-weight: bold\"> \n            Lieferschein {{doc.name}} \n        </h4>\n    </div> \n            <div style=\"display: block\">\n                    <div style=\"float: left; background-color: white\">\n\n<p style=\"color: black; margin-left: 90px; line-height: 12px;\">\n        <strong> Datum: </strong>           <br>\n         <strong> Auftrags Nr.: </strong>   <br>\n        Ihr Sachbearbeiter(in):             <br>\n        Telefon-Nr.:                        <br>\n        Ihr Ansprechpartner                 <br>\n        <strong> Objekt: </strong>          <br>\n        Bestellung vom:                     <br>\n        Ihre Anfrage-Nr.:                   <br><br>\n        <strong> Liefertermin: </strong>    <br>\n</p>\n</div>\n\n<div style=\"float: left; background-color: white\">\n<p style=\"color: black; margin-left: 25px; line-height: 12px;\">\n     <strong>   {{ doc.get_formatted('posting_date') }} </strong>     <br> \n     \n{% set sales_ord = doc.items[0].sales_order %}\n{% set sales_doc = frappe.get_doc(\"Sales Order\", sales_ord) %}\n{% set prev_name = sales_doc.items[0].prevdoc_docname %}\n{% set del = sales_doc.items[0].delivery_date %}\n{% set trans = sales_doc.items[0].transaction_date %}\n\n<strong> {{prev_name}} </strong> \n     <br>\n{% if doc.contact_person %}\n        {% set contact = frappe.get_doc(\"Contact\", doc.contact_person) %}\n        {{contact.salutation or \"\" }} {{contact.first_name}} {{contact.last_name}} <br>\n  {% if contact.mobile_no %}\n    {{contact.mobile_no}}\n  {% endif %}<br>\n{% else %}\n  <br><br>\n{% endif %}\n{% set owner = frappe.get_doc(\"User\", doc.owner ) %}{{ owner.full_name }}<br>\n<strong>gem\u00e4ss Offerte {{prev_name}} </strong><br>\n{{ frappe.utils.get_datetime(trans).strftime('%d.%m.%Y') }}<br>\n{{doc.title}} <br>\n<br>\n<strong>{{ frappe.utils.get_datetime(del).strftime('%d.%m.%Y') }}</strong> \n</p>\n</div>  \n            </div>\n    </div>\n<!-- tier 1 right div -->\n\n</div> <!-- finish tag complete part-->\n\n\n\n\n\n\n\n<table class=\"table table table-bordered\", style=\"width: 100%\" >\n    <col width=\"5.9%\">\n    <col width=\"8.3%\">\n    <col width=\"5.5%\">\n    <col width=\"61%\">\n    <col width=\"19.3%\">\n  <tr>\n    <td style=\"text-align: center; font-weight: bold\">Pos.</th>\n    <td style=\"text-align: center; font-weight: bold\">Anz.</th>\n    <td style=\"text-align: left; font-weight: bold\">Einh.</th>\n    <td style=\"text-align: left; font-weight: bold\">Bezeichnung</th>\n    <td style=\"text-align: center; font-weight: bold\">Gewicht [kg]</th>\n \n  </tr>\n     {% for n in doc.items %}\n  <tr>\n                <td style=\"text-align: center\"><br>{{ loop.index }}</td>\n                <td style=\"text-align: right\"><br>{{ n.qty }}</td>\n                <td style=\"text-align: left\"><br>{{n.uom}}.</td>\n                <td style=\"text-align: left\"><br> {% set item = frappe.get_doc(\"Item\", n.item_code) %}\n                {{ n.item_name }}<br>\n                {% if not n.item_name in n.description %}{{ n.description }}{% endif %}\n                {% if item.material %}Material: {{ item.material }}<br>{% endif %}\n                {% if item.drawing_no %}Plan Nr. {{ item.drawing_no }}{% endif %}\n</td>\n<td style=\"text-align: right\"><br>\n{{ \"%.3f\"|format(n.weight) }}\n</td>\n </tr>\n    {% endfor %}\n<tr>\n    <td></td>\n    <td></td>\n    <td ></td>\n    <td style=\"text-left: left;\"> \n\n    <table> \n   \n<tr>\n        <td style=\"width: 33%; border: white solid 1px; line-height: 25px\">\n        Gebindekontrolle  <br> <br>\n        Kartonschachtel   <br>\n        EUR Palette       <br>\n        Rahmen            <br>\n        Deckel            <br>\n        </td>\n        <td style=\"width: 33%; border: white solid 1px; line-height: 25px\">\n        Geliefert         <br> <br>\n        _________         <br>\n        _________         <br>\n        _________         <br>\n        _________         <br>\n         </td>\n        <td style=\"width: 33%; border: white solid 1px; line-height: 25px\">\n        Ausgetauscht    <br> <br> <br>\n        _________       <br>\n        _________       <br>\n        _________       <br> \n        </td>\n</tr>\n    </table>\n    \n    \n    <table><tr>\n         <td style=\"width: 100%; border: white solid 1px; line-height: 25px;\">\n            Datum: _____________________________ <br> \n            Name in Blockschrift: __________________ <br>\n            Visum: _____________________________ <br>\n            LKW - Kennzeichen: ___________________ <br>\n    </td>\n    </tr> </table>\n    \n    \n    \n    \n    \n    \n\n<!--START DIV TAG 1 STAMP --> \n<div id=\"myDiv1\" style=\"display: block; margin-top: 25px; width: 70%\"> \n         <div style=\"float: right; width: 31.5%; border-bottom: solid red; border-top: solid red; border-right: solid red;; height: 70px\">\n        \n            <div style=\"border-bottom: solid red\"> \n                <p style=\"color: black; font-size: 17.5px; text-align: center\"> <!-- 23.11.19 --> </p>  <!-- add form in system to accces-->\n            </div> \n\n            <div style=\"\">\n                <p style=\"color: black; font-size: 17.5px; text-align: center\"> <!-- B.Tester --> </p>  <!-- add form in system to accces-->\n            </div>\n          </div> \n\n          <div style=\"float: right; width: 26.3%; border-bottom: solid red; border-top: solid red; border-right: solid red; height: 70px\">\n            <div style=\"border-bottom: solid red\">\n                <p style=\"color: red; font-size: 17.5px; text-align: center\"> Datum </p>\n            </div>\n\n            <div style=\"\">\n                <p style=\"color: red; font-size: 17.5px; text-align: center\"> Visum </p>\n            </div>\n         </div>\n\n\n         <div style=\"float: right; width: 42.1%; border: solid red; height: 70px\">\n            <p style=\"margin-top: 15px; color: red; font-size: 25px;  text-align: center\"> Gez\u00e4hlt  </p>\n         </div>\n\n</div> <!-- END DIV TAG1 TOTAL-->\n\n<div> <span style=\"color: white\"> Test </span> </div>\n\n\n<!--START DIV TAG 2 STAMP --> \n<div id=\"myDiv2\" style=\"display: block; margin-top: 25px; width: 70%\"> \n         <div style=\"float: right; width: 31.5%; border-bottom: solid red; border-top: solid red; border-right: solid red;; height: 70px\">\n        \n            <div style=\"border-bottom: solid red\"> \n                <p style=\"color: black; font-size: 17.5px; text-align: center\"> <!-- 22.10.19 --> </p>  <!-- add form in system to accces-->\n            </div> \n\n            <div style=\"\">\n                <p style=\"color: Black; font-size: 17.5px; text-align: center\"> <!-- M.Muster --> </p> <!-- add form in system to accces-->\n            </div>\n   \n          </div> \n\n          <div style=\"float: right; width: 26.3%; border-bottom: solid red; border-top: solid red; border-right: solid red; height: 70px\">\n            <div style=\"border-bottom: solid red\">\n                <p style=\"color: red; font-size: 17.5px; text-align: center\"> Datum </p> <!-- add form in system to accces-->\n            </div>\n\n            <div style=\"\">\n                <p style=\"color: red; font-size: 17.5px; text-align: center\"> Visum </p> <!-- add form in system to accces-->\n            </div>\n         </div>\n\n\n         <div style=\"float: right; width: 42.1%; border: solid red; height: 70px\">\n            <p style=\"margin-top: 15px; color: red; font-size: 20px;  text-align: center\"> Kontrolliert  </p>\n         </div>\n\n</div> <!-- END DIV TAG2 TOTAL-->\n</td>\n    <td></td>\n  </tr>\n</table>\n<!-- quotation part --> \n<div style=\"display: block\">\n\n<div style=\"float: left; width: 40%; background-color: white;\"> \n    <p style=\"line-height: 15px;\">\n        Ware in einwandfreiem Zustand erhalten:         <br><br> \n        Beanstandungen k\u00f6nnen nur innerhalb         <br> \n        von 5 Tagen ber\u00fccksichtigt werden.\n    </p>\n</div>\n\n\n<div style=\"float: left; width: 30%; background-color: white;\"> \n    <p style=\"line-height: 15px;\">\n                            <br>    \n        Datum: ..........................       <br> <br>\n        Unterschrift: ..........................................    <br> \n    \n    </p>\n</div>\n\n\n\n\n<div style=\"float: right; width: 30%; background-color: white\">\n\n\n<div style=\"float: left; text-align: left; background-color: white\"> \n    <p id=\"simpleWhite\"> \n        <strong> Total Gewicht </strong> <br> \n</p>\n    \n        </div>\n\n<div  style=\"float: right; text-align: left; background-color: white\"> \n    <p id=\"simple\"> \n        <strong>{{ \"%.3f\"|format(doc.total_net_weight) }} <br> \n    </p>\n    \n</div>\n\n    </div>\n\n</div>\n\n\n<!-- page footer -->\n<div id=\"footer-html\" class=\"visible-pdf\">\n  {% if letter_head %}\n    {{ letter_head.footer }}\n  {% else %}\n    <p>No letter head found. Please define a letter head under print settings and mark it as default</p>\n  {% endif %}\n  <p style=\"font-size: 6pt !important; text-align: right\">\n    {{ _(\"Page\") }} <span class=\"page\"></span> {{ _(\"of\") }} <span class=\"topage\"></span>\n  </p>\n</div>",
